{
    "id": "nlpcraft.timer.ex",
    "name": "Timer Example Model",
    "version": "1.0",
    "description": "Timer example model.",
    "vendorName": "NlpCraft",
    "vendorUrl": "https://www.nlpcraft.org",
    "vendorContact": "Support",
    "vendorEmail": "info@nlpcraft.org",
    "docsUrl": "https://www.nlpcraft.org",
    "examples": [
        "Ping me in 3 minutes",
        "Buzz me in an hour and 15mins",
        "Set my alarm for 30s"
    ],
    "elements": [
        {
            "id": "x:timer",
            "description": "Timer token indicator.",
            "synonyms": [
                "{ping|buzz|wake|call|hit} {me|up|me up|*}",
                "{set|*} {my|*} {wake|wake up|*} {alarm|timer|clock|buzzer|call} {up|*}"
            ]
        }
    ]
}